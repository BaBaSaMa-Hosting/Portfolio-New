{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "target": "ES5",
    "noImplicitAny": false,
    "sourceMap": true,
    "outDir": "dist/",
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@icons/*": [
        "src/icons/*"
      ],
      "@classes/*": [
        "src/classes/*"
      ],
      "@src/*": [
        "src/*"
      ],
    },
    "jsx": "preserve"
  },
  "exclude": [
    "dist",
    "node_modules"
  ]
}